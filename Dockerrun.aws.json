{
    "AWSEBDockerrunVersion": "1",
    "Ports": [
      {
        "ContainerPort": "80"
      }
    ],
    "Volumes": [
      {
        "HostDirectory": "/wp-content",
        "ContainerDirectory": "/var/www/html/wp-content"
      }
    ],
    "Entrypoint": "docker-entrypoint.sh",
    "Command": "php-fpm --user ec2-user:ec2-user"
  }